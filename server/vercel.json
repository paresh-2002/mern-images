{
  "installCommand":"npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
    { "src": "./src/index.js", "use": "@vercel/node" },
    { "src": "src", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "./src/index.js" },
    { "src": "/(.*)", "dest": "/" }  
  ]
}